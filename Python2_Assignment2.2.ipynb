   "source": [
    "### 2.2 Program to print a pattern using nested for loop"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\r\n",
      "* \r\n",
      "* * \r\n",
      "* * * \r\n",
      "* * * * \r\n",
      "* * * * * \r\n",
      "* * * * \r\n",
      "* * * \r\n",
      "* * \r\n",
      "* \r\n"
     ]
    }
   ],
   "source": [
    "n=5\n",
    "for rows in range(0,n):\n",
    "    for cols in range(0,rows):\n",
    "        print(\"*\", end=' ' )\n",
    "    print(\"\\r\")\n",
    "    \n",
    "for rows1 in range(n,0,-1):\n",
    "    for cols in range(0,rows1):\n",
    "        print(\"*\", end=' ')\n",
    "    print(\"\\r\")"
   ]
  },
